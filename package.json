{
  "name": "simple-crud",
  "version": "0.0.1",
  "description": "Simple Crud Node.js + Express",
  "main": "app.ts",
  "scripts": {
    "test": "npm test",
    "start": "nodemon dist/server/app.js",
    "dev": "nodemon --config nodemon.json server/app.ts",
    "build": "rimraf dist && tsc",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add dist",
    "lint": "eslint . --ext .ts"
  },
  "pre-commit": [
    "ts.check",
    "build",
    "add-build"
  ],
  "keywords": [
    "simple-crud"
  ],
  "author": "Arief Zainuri",
  "license": "ISC",
  "dependencies": {
    "-": "^0.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "mongodb": "^6.3.0",
    "mongoose": "^8.1.2",
    "nodemon": "^3.0.3",
    "save-dev": "^0.0.1-security",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.21",
    "@types/node": "^20.8.10",
    "pre-commit": "^1.2.2",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2"
  }
}
